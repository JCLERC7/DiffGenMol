{
    "name": "DiffGenMol-Selfies",
    "n_gpu": 1,
    "data_loader": {
        "type": "GuacamolDataLoader",
        "args": {
            "dataset_size": 2500,
            "max_smiles_size": 30,
            "batch_size": 16,
            "num_classes": 10,
            "type_property": "Weight",
            "shuffle": true
        }
    },
    "trainer": {
        "type": "Trainer1D",
        "args": {
            "timesteps": 300,
            "epochs": 1000,
            "lr": 0.0001,
            "unet_dim": 16,
            "unet_channels": 1,
            "unet_cond_drop_prob": 0.5,
            "results_folder": "logger/",
            "save_dir": "saved/",
            "save_period": 1,
            "tensorboard": true
        }
    }
}